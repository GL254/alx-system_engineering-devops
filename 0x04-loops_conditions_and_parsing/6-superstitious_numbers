# The script diplays numbers from 1-20, it also diplays
#+ for the 4 iteration "bad luck from China", for the 9 
#+ iteration "bad luck from Japan" and for the 17 iteration
>>>>>>> 6711d67b4c56038e80f0480d3f8da6d0eb08da7e
#+ "bad luck from Italy"

num=1

while [ $num -le 20 ]
do
	case $num in
		4)
			echo $num
			echo "bad luck from China"
			;;
		9)
			echo $num
			echo "bad luck from Japan"
			;;
		17)
			echo $num
			echo "bad luck from Italy"
			;;
		*)
			echo $num
			;;
	esac
	((num=num+1))
done
